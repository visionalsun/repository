	<!-- RTM：生产|压片版、OEM：发行|随机版、RVL：[伪]正式版、EVAL：评估版、RTL：零售版、 WX：正式版、WB：测试版、PRE：家庭版、PRO：专业版 -->
	<!-- <version>1.0.1</version> -->
	<packaging>jar</packaging>
	
	
	<parent>
		<groupId>com.gitee.visionalsun</groupId>
